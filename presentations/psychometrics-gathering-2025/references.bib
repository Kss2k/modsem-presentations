	urldate = {2023-04-30},
	journal = {Archives of Internal Medicine},
	author = {Shanafelt, Tait D. and Boone, Sonja and Tan, Litjen and Dyrbye, Lotte N. and Sotile, Wayne and Satele, Daniel and West, Colin P. and Sloan, Jeff and Oreskovich, Michael R.},
	month = oct,
	year = {2012},
	pages = {1377--1385},
	file = {Fulltekst:/home/kjell/Jurism/storage/4NP2G23N/Shanafelt et al. - 2012 - Burnout and Satisfaction With Work-Life Balance Am.pdf:application/pdf;Snapshot:/home/kjell/Jurism/storage/GX3HHZR2/1351351.html:text/html},
}

